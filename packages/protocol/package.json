{
  "name": "@evmts/smithers-protocol",
  "version": "1.0.0",
  "description": "WebSocket protocol types for Smithers CLI and Desktop app communication",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "bun run scripts/build.ts",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@evmts/smithers": "workspace:*"
  },
  "devDependencies": {
    "typescript": "^5.7.2"
  },
  "author": "William Cory",
  "license": "MIT"
}
