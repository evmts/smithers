{
  "name": "@evmts/smithers",
  "version": "1.0.0",
  "description": "Build AI agents in Solid. Declarative JSX components for orchestrating Claude agents with signal-based state management and parallel execution.",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./jsx-runtime": {
      "types": "./dist/solid-js-jsx-runtime/jsx-runtime.d.ts",
      "import": "./dist/solid-js-jsx-runtime/jsx-runtime.js"
    },
    "./jsx-dev-runtime": {
      "types": "./dist/solid-js-jsx-runtime/jsx-dev-runtime.d.ts",
      "import": "./dist/solid-js-jsx-runtime/jsx-dev-runtime.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "bun test",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "peerDependencies": {
    "@evmts/smithers": "workspace:*",
    "solid-js": "^1.9.10"
  },
  "devDependencies": {
    "@evmts/smithers-core": "workspace:*",
    "@evmts/smithers": "workspace:*",
    "@types/bun": "latest",
    "solid-js": "^1.9.10",
    "typescript": "^5.7.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/evmts/smithers.git",
    "directory": "packages/smithers-solid"
  },
  "homepage": "https://github.com/evmts/smithers#readme",
  "bugs": {
    "url": "https://github.com/evmts/smithers/issues"
  },
  "keywords": [
    "ai",
    "agent",
    "solid",
    "solidjs",
    "jsx",
    "llm",
    "claude",
    "anthropic",
    "smithers"
  ],
  "author": "William Cory",
  "license": "MIT"
}
