{
  "name": "@evmts/smithers-cli",
  "version": "1.0.0",
  "description": "CLI for running Smithers AI agents",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "smithers": "dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bun run scripts/build.ts",
    "dev": "bun run src/index.ts",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@evmts/smithers": "workspace:*",
    "@evmts/smithers-protocol": "workspace:*",
    "@mdx-js/mdx": "^3.1.1",
    "commander": "^12.1.0",
    "ora": "^8.1.1",
    "picocolors": "^1.1.1",
    "react": "^19.2.3",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/ws": "^8.5.13",
    "typescript": "^5.7.2"
  },
  "author": "William Cory",
  "license": "MIT"
}
