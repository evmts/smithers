{
  "name": "@evmts/smithers-cli",
  "version": "1.0.0",
  "description": "CLI for running Smithers AI agents",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./display": {
      "types": "./dist/display.d.ts",
      "import": "./dist/display.js"
    },
    "./loader": {
      "types": "./dist/loader.d.ts",
      "import": "./dist/loader.js"
    },
    "./config": {
      "types": "./dist/config.d.ts",
      "import": "./dist/config.js"
    },
    "./props": {
      "types": "./dist/props.d.ts",
      "import": "./dist/props.js"
    },
    "./interactive": {
      "types": "./dist/interactive.d.ts",
      "import": "./dist/interactive.js"
    },
    "./testing": {
      "types": "./dist/testing.d.ts",
      "import": "./dist/testing.js"
    }
  },
  "bin": {
    "smithers": "dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bun run scripts/build.ts",
    "dev": "bun run src/index.ts",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@evmts/smithers": "workspace:*",
    "@evmts/smithers-protocol": "workspace:*",
    "@mdx-js/mdx": "^3.1.1",
    "commander": "^12.1.0",
    "ora": "^8.1.1",
    "picocolors": "^1.1.1",
    "solid-js": "^1.9.10",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/ws": "^8.5.13",
    "typescript": "^5.7.2"
  },
  "author": "William Cory",
  "license": "MIT"
}
