{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "rootDir": "..",
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "noUnusedLocals": false,
    "noUnusedParameters": false
  },
  "include": [
    "./**/*",
    "../src/**/*"
  ],
  "exclude": [
    "../node_modules",
    "../dist"
  ]
}
