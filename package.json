{
  "name": "smithers-orchestrator",
  "version": "0.1.5",
  "description": "Build AI agents with Solid.js - Declarative JSX for Claude orchestration",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "smithers": "./bin/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./core": {
      "types": "./dist/core/index.d.ts",
      "import": "./dist/core/index.js"
    },
    "./solid": {
      "types": "./dist/solid/index.d.ts",
      "import": "./dist/solid/index.js"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "import": "./dist/components/index.js"
    },
    "./components/*": {
      "types": "./dist/components/*.d.ts",
      "import": "./dist/components/*.js"
    },
    "./orchestrator": {
      "types": "./dist/orchestrator/components/index.d.ts",
      "import": "./dist/orchestrator/components/index.js"
    },
    "./orchestrator/*": {
      "types": "./dist/orchestrator/components/*.d.ts",
      "import": "./dist/orchestrator/components/*.js"
    },
    "./db": {
      "types": "./dist/orchestrator/db/index.d.ts",
      "import": "./dist/orchestrator/db/index.js"
    },
    "./debug": {
      "types": "./dist/debug/index.d.ts",
      "import": "./dist/debug/index.js"
    },
    "./renderer": {
      "import": "./src/solid/renderer.js"
    }
  },
  "files": [
    "dist",
    "bin",
    "skills",
    "templates",
    "plugin.json",
    "postinstall.cjs",
    "README.md"
  ],
  "scripts": {
    "build": "tsc && bun build bin/cli.ts --outdir bin --target node",
    "build:cli": "bun build bin/cli.ts --outdir bin --target node",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "postinstall": "node postinstall.cjs"
  },
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.1.76",
    "@anthropic-ai/sdk": "^0.71.2",
    "@electric-sql/pglite": "^0.3.15",
    "commander": "^12.0.0",
    "solid-js": "^1.9.10",
    "zod": "^4.3.5",
    "zustand": "^5.0.10"
  },
  "peerDependencies": {
    "solid-js": "^1.9.10",
    "bun": ">=1.0.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^25.0.9",
    "happy-dom": "^20.3.1",
    "typescript": "^5.7.2",
    "vite-plugin-solid": "^2.11.10",
    "vitest": "^4.0.17"
  },
  "keywords": [
    "ai",
    "agent",
    "solid",
    "solidjs",
    "jsx",
    "llm",
    "claude",
    "anthropic",
    "orchestration",
    "multi-agent",
    "workflow"
  ],
  "author": "William Cory",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/evmts/smithers.git"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
