{
  "name": "smithers-orchestrator",
  "version": "1.0.0",
  "description": "CLI tool and Claude Code plugin for multi-agent AI orchestration with Smithers framework",
  "author": "William Cory",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/evmts/smithers-orchestrator.git"
  },
  "homepage": "https://github.com/evmts/smithers#readme",
  "keywords": [
    "ai",
    "agents",
    "orchestration",
    "multi-agent",
    "smithers",
    "claude",
    "workflow",
    "jsx"
  ],
  "type": "module",
  "bin": {
    "smithers-orchestrator": "./bin/cli.js"
  },
  "files": [
    "bin/",
    "dist/",
    "plugin.json",
    "skills/",
    "templates/",
    "postinstall.cjs"
  ],
  "scripts": {
    "build": "bun build bin/cli.ts --outdir bin --target node",
    "postinstall": "node postinstall.cjs",
    "dev": "bun run bin/cli.ts",
    "test": "bun test"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.30.0",
    "@electric-sql/pglite": "^0.3.15",
    "commander": "^12.0.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "peerDependencies": {
    "bun": ">=1.0.0"
  }
}
